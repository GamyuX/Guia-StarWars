<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="141dp"
        android:layout_height="139dp"
        app:srcCompat="@drawable/character_icon"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="533dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="148dp"
        android:layout_height="212dp"
        app:srcCompat="@drawable/planet_icon"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="34dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="146dp"
        android:layout_height="235dp"
        app:srcCompat="@drawable/spaceship_icon"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="246dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonPlanet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Planeta"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="107dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonVeicles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="veiculos"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="332dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonPerson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Personagens"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="574dp"
        tools:ignore="MissingConstraints"/>

</androidx.constraintlayout.widget.ConstraintLayout>

